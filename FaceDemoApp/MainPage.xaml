<Page
    x:Class="FaceDemoApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FaceDemoApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <CaptureElement x:Name="ctlCameraFeed" HorizontalAlignment="Left" Height="508" Margin="80,96,0,0" VerticalAlignment="Top" Width="582"/>
        <TextBlock x:Name="lblName" HorizontalAlignment="Left" Margin="781,296,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="48" Height="74" Width="405"/>
        <Button x:Name="btnStart" Content="Button" HorizontalAlignment="Left" Height="126" Margin="781,114,0,0" VerticalAlignment="Top" Width="405" FontSize="48"/>
        <Button x:Name="btnAdd" Content="Button" HorizontalAlignment="Left" Height="126" Margin="781,537,0,0" VerticalAlignment="Top" Width="405" FontSize="48"/>
        <TextBlock x:Name="lblAge" HorizontalAlignment="Left" Height="64" Margin="889,375,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="204" FontSize="36"/>
        <TextBlock x:Name="lblRate" HorizontalAlignment="Left" Height="64" Margin="889,439,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="204" FontSize="36"/>

    </Grid>
</Page>
